/********************************************************************/
/*						今のイベントのOBJを消す						*/
//
//	二人組みのイベントが、二人で会話する流れなので、
//	片方だけを移動することは出来ない。
//	なので、暗転中にOBJ消して、専用イベントの流れにもっていく
/********************************************************************/
ev_t07r0201_event_obj_del:
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_RIVAL,	t07r0201_event01_del
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_SUPPORT,	t07r0201_event02_del
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_DOCTOR,	t07r0201_event03_del
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_MAMA,		t07r0201_event04_del
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_LE1,		t07r0201_event05_del
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_LE2,		t07r0201_event06_del
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_LE3,		t07r0201_event07_del
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_LE4,		t07r0201_event08_del
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_LE5,		t07r0201_event09_del
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_LE6,		t07r0201_event10_del
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_LE7,		t07r0201_event11_del
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_LE8,		t07r0201_event12_del
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_CHAMP,	t07r0201_event13_del
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_BIG_LE,	t07r0201_event14_del
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_LE_LE,	t07r0201_event15_del
	_RET

t07r0201_event01_del:
	/*ライバル専用は去っていくが、その時は判別ワークに0xffが入っているので、ここにはこないはず*/
	_OBJ_DEL		T07R0201_RIVAL
	_RET

