	/*終了キーチェック*/
	_CONT_AB_KEY	SCWK_ANSWER
	_IFVAL_JUMP		SCWK_ANSWER,EQ,1,ev_t07r0201_bgm_end

	_DEBUG_WATCH_WORK	SCWK_TEMP3
	//_IFVAL_JUMP		SCWK_TEMP3,EQ,1180,ev_t07r0201_bgm_end
	//_IFVAL_JUMP		SCWK_TEMP3,EQ,512,ev_t07r0201_bgm_end
	_IFVAL_JUMP		SCWK_TEMP3,EQ,600,ev_t07r0201_bgm_end

	_JUMP			ev_t07r0201_bgm_wait
