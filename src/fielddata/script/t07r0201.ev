/********************************************************************/
/*						FLAG_CHANGE_LABEL 							*/
/********************************************************************/
ev_t07r0201_flag_change:
	_INIT_VILLA

	//WKOBJCHRにサポートのキャラコードをセットする
	_GET_MY_SEX		LOCALWORK0
	_IFVAL_CALL		LOCALWORK0,EQ,0,t07r0201_male_flag_change_01	//サポート女
	_IFVAL_CALL		LOCALWORK0,EQ,1,t07r0201_female_flag_change_01	//サポート男

	/*POSでOBJが表示されていたら何もしない*/
	_IF_FLAGON_JUMP	SYS_FLAG_T07R0201_POS_APPEAR,ev_t07r0201_flag_change_end
	
	/*電源投入時に来客するNPCが、いる、いないを決定*/
	/*1日1回来客が変更される(ev_timeでupdateが呼ばれる)*/

	/*全員消す(IN=室内)*/
	_FLAG_SET		FV_VILLA_IN_RIVAL
	_FLAG_SET		FV_VILLA_IN_SUPPORT
	_FLAG_SET		FV_VILLA_IN_DOCTOR
	_FLAG_SET		FV_VILLA_IN_MAMA
	_FLAG_SET		FV_VILLA_IN_LEADER1
	_FLAG_SET		FV_VILLA_IN_LEADER2
	_FLAG_SET		FV_VILLA_IN_LEADER3
	_FLAG_SET		FV_VILLA_IN_LEADER4
	_FLAG_SET		FV_VILLA_IN_LEADER5
	_FLAG_SET		FV_VILLA_IN_LEADER6
	_FLAG_SET		FV_VILLA_IN_LEADER7
	_FLAG_SET		FV_VILLA_IN_LEADER8
	_FLAG_SET		FV_VILLA_IN_CHAMP
	_FLAG_SET		FV_VILLA_IN_BIGFOUR3

	/*イベントが発生しているかチェック*/
	_IFVAL_JUMP		SYS_WORK_EVENT_T07R0201,EQ,0xff,ev_t07r0201_event_not

	/*POSイベント用にワークに格納*/
	_LDWK			LOCALWORK15,SYS_WORK_EVENT_T07R0201

	/*イベントに必要なOBJを表示する*/
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_RIVAL,	t07r0201_event01
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_SUPPORT,	t07r0201_event02
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_DOCTOR,	t07r0201_event03
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_MAMA,		t07r0201_event04
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_LE1,		t07r0201_event05
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_LE2,		t07r0201_event06
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_LE3,		t07r0201_event07
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_LE4,		t07r0201_event08
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_LE5,		t07r0201_event09
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_LE6,		t07r0201_event10
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_LE7,		t07r0201_event11
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_LE8,		t07r0201_event12
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_CHAMP,	t07r0201_event13
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_BIG_LE,	t07r0201_event14
	_IFVAL_CALL		SYS_WORK_EVENT_T07R0201,EQ,T07R0201_EVENT_LE_LE,	t07r0201_event15

	_END

/*POSでOBJが表示されていたら何もしない*/
ev_t07r0201_flag_change_end:
	_END

